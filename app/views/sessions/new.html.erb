<%= form_tag(:sessions, method: :post) do %>
  <%= label_tag(:email, nil, class: 'label') %>
  <%= email_field_tag(:email, nil, class: 'input') %>
  <%= label_tag(:password, nil, class: 'label') %>
  <%= password_field_tag(:password, nil, class: 'input') %>
  <%= submit_tag('Sign In', class: 'button') %>
<% end %>
<script>
  (function () {
    setupWarningMessage();
  })();
</script>
